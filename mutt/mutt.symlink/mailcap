# parse html via lynx
text/html;          links -codepage utf-8 -dump %s; copiousoutput;

# open files using Mac OS X "open" and default program
application/pdf;    open %s; nametemplate=%s.pdf; copiousoutput;
image/jpeg;         open %s; nametemplate=%s.jpeg; copiousoutput;
image/png;          open %s; nametemplate=%s.png; copiousoutput;
image/gif;          open %s; nametemplate=%s.gif; copiousoutput;
